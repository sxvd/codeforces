#include <bits/stdc++.h>

using namespace std;

using ll = long long;
using ld = long double;

template <typename T> void print(T val) { cout << val << '\n'; }

template <typename Iterator> void print(Iterator begin, Iterator end) {
  for (auto it = begin; it != end; ++it) {
    cout << *it << ' ';
  }

  cout << '\n';
}

void solve() {
  int x;
  cin >> x;
  print(1 << (x-3));
}

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

#ifdef LOCAL
  freopen("input.txt", "r", stdin);
  freopen("output.txt", "w", stdout);
#endif

  solve();

  return 0;
}

